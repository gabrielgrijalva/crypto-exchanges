{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "object-shorthand": "off",
        "prefer-destructuring": "off",
        "max-len": [
            "error",
            {
                "ignoreComments": true
            }
        ],
        "func-names": "off"
    }
}